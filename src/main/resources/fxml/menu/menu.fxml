<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="view" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="5.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="homeButton" mnemonicParsing="false" onAction="#onHomeAction" style="-fx-font-family: Montserrat;" text="%app.menu.home" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button fx:id="productButton" mnemonicParsing="false" style="-fx-font-family: Montserrat; -fx-font-weight: bold;" text="Func" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button mnemonicParsing="false" style="-fx-font-family: Montserrat;" text="Func" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button mnemonicParsing="false" text="Func" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button fx:id="configButton" mnemonicParsing="false" text="%app.menu.config" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
      <Button fx:id="testingButton" mnemonicParsing="false" onAction="#onTestingAction" text="Testing" HBox.hgrow="ALWAYS">
         <graphic>
            <ImageView pickOnBounds="true" preserveRatio="true" />
         </graphic>
      </Button>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
